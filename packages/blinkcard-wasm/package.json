    "./size-manifest.json": "./dist/size-manifest.json",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "types"
  ],
  "devDependencies": {
    "@microblink/tsconfig": "workspace:*",
    "@microblink/wasm-common": "workspace:*",
    "dotenv-cli": "^8.0.0",
    "prettier": "^3.8.1",
    "prettier-plugin-jsdoc": "^1.3.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.21.0",
    "type-fest": "^4.35.0",
    "typedoc": "^0.28.7",
    "typedoc-plugin-markdown": "^4.7.0",
    "typescript": "5.8.3",
    "zx": "^8.8.5"
  },
  "keywords": [
    "aml",
    "blinkcard",
    "compliance",
    "computer-vision",
    "card-scanner",
    "card-verification",
    "fraud-detection",
    "frontend",
    "card-check",
    "microblink",
    "ocr",
    "typescript",
    "verification",
    "credit-card",
