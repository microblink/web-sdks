{
  "name": "@microblink/blinkcard-wasm",
  "private": true,
  "version": "3000.0.1",
  "scripts": {
    "build": "tsc",
    "build:publish": "tsc",
    "clean": "rimraf .turbo types",
    "docs:api": "typedoc",
    "lint": "pnpm lint:types",
    "lint:types": "tsc --emitDeclarationOnly false --noEmit"
  },
  "type": "module",
  "main": "src/index.ts",
  "module": "./dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
