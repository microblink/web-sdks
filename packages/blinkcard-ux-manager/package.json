{
  "name": "@microblink/blinkcard-ux-manager",
  "description": "BlinkCard UX Manager provides user feedback based on the blinkcard process results.",
  "version": "3000.0.1",
  "author": "Microblink",
  "scripts": {
    "build": "concurrently pnpm:build:js pnpm:build:types",
    "build:js": "vite build --mode ${VITE_BUILD_MODE:-production}",
    "build:js-dev": "vite build --mode development",
    "build:publish": "pnpm build && pnpm type-rollup",
    "build:types": "tsc",
    "clean": "rimraf .turbo types dist publish public/resources/* *.tgz",
    "docs:api": "typedoc",
    "lint": "concurrently pnpm:lint:types pnpm:lint:eslint",
    "lint:eslint": "eslint ./src --max-warnings 0",
    "lint:types": "tsc --emitDeclarationOnly false --noEmit",
    "pack-check": "pnpm pack | tail -n1 | xargs attw --profile esm-only && publint publish",
    "prepack": "pnpm prepare-publish",
    "prepare-publish": "tsx scripts/prepare-publish.mts",
    "publish-test": "pnpm build:publish && pnpm pack-check",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-rollup": "api-extractor run --local",
    "watch": "concurrently pnpm:watch:js pnpm:watch:types",
    "watch:js": "pnpm run build:js --watch --mode development",
    "watch:types": "pnpm run build:types --watch"
  },
  "type": "module",
  "main": "./dist/blinkcard-ux-manager.js",
  "module": "./dist/blinkcard-ux-manager.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/blinkcard-ux-manager.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "directory": "publish",
    "linkDirectory": false
  },
  "files": [
    "dist",
    "types"
  ],
  "dependencies": {
    "@microblink/analytics": "workspace:^",
    "@microblink/blinkcard-core": "workspace:^",
    "@microblink/camera-manager": "workspace:^",
    "@microblink/ux-common": "workspace:^",
    "@solid-primitives/keyed": "^1.5.2",
    "clsx": "^2.1.1",
    "merge-anything": "^6.0.3",
    "perfect-debounce": "^2.1.0",
    "solid-motionone": "^1.0.3",
    "ts-pattern": "^5.6.2",
    "type-fest": "^4.35.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@microblink/feedback-stabilizer": "workspace:^",
    "@microblink/repo-utils": "workspace:*",
    "@microblink/shared-components": "workspace:^",
    "@microblink/test-utils": "workspace:*",
    "@microblink/tsconfig": "workspace:*",
    "@microblink/unocss-config": "workspace:*",
    "@microsoft/api-extractor": "^7.52.4",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@unocss/eslint-config": "^65.5.0",
    "@unocss/preset-uno": "^65.5.0",
    "@vitest/browser": "^3.0.8",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-solid": "0.14.5",
    "prettier-plugin-classnames": "0.9.0",
    "publint": "^0.3.12",
    "rimraf": "^6.0.1",
    "sass": "^1.85.0",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.7",
    "typedoc-plugin-markdown": "^4.7.0",
    "typescript": "5.8.3",
    "unocss": "^66.5.7",
    "vite": "^6.2.1",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-externalize-dependencies": "1.0.1",
    "vite-plugin-solid": "^2.11.10",
    "vite-plugin-solid-svg": "0.8.1",
    "vitest": "^3.0.8",
    "write-package": "^7.1.0",
    "zx": "^8.8.5"
  },
  "peerDependencies": {
    "@ark-ui/solid": "^5.30.0",
    "solid-js": "^1.9.11",
    "solid-zustand": "^1.8.1"
  },
  "keywords": [
    "microblink",
    "blinkcard",
    "blinkcard-ux-manager",
    "card-scanner",
    "card-recognition",
    "ux",
    "ui",
    "frontend",
    "browser",
    "camera",
    "typescript",
    "sdk"
  ],
  "license": "ISC"
}
